# Server Configuration
PORT=4000
NODE_ENV=development

# Data Directory Path (relative to project root or absolute path)
DATA_DIR=../data

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_DEFAULT_MODEL=deepseek-r1:14b

# Azure DevOps Integration
AZURE_DEVOPS_URL=https://dev.azure.com/yourorganization
AZURE_DEVOPS_PAT=your_personal_access_token
AZURE_DEVOPS_PROJECT=your_project_name
AZURE_DEVOPS_TEAM=your_team_name

# File Upload Configuration
MAX_UPLOAD_SIZE_MB=10
ALLOWED_FILE_TYPES=pdf,docx,md

# Socket.IO Configuration
SOCKET_CORS_ORIGIN=*
